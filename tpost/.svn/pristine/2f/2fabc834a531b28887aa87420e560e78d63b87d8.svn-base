@charset "utf-8";
/* CSS Document */
html 	{/*min-width:1260px;*/}
body    {background:#f2f2f2 ;}

#wrap {width:100%; height:100% auto;background-color:#f2f2f2 ;}
#wrap.bg{background:url("../images/body.png") repeat-y;}

/* header */
#header {position:relative; width:100%;background:#fff; }
#header h1 {position:absolute;background: #db4550; width: 220px; height: 100%;}
#header h1 a{color:#fff; width: 6em; font-size:21px; line-height: 24px; display: block;text-align: center; margin: 33px auto 0;}

/* 사용자정보 */
#header .userInfo {position:absolute; right:20px; top:30px; font-size:14px; font-weight: 200;color:#243c54;}
#header .userInfo a {color:#243c54; font-size:14px;}
#header .userInfo li {display:inline-block; float: left; margin: 0 10px;}
#header .userInfo li a:before{content: "";width: 20px; height: 20px;display: inline-block; margin: 0 8px -10px 0; float:left;}
#header .userInfo li span:before{content: "";width: 36px; height: 36px;display: inline-block;	background: #ccc; margin: -9px 0 0 8px; float:right;}
#header .userInfo li.info_pw a:before{background:url("../images/icon-pwchange.png") no-repeat center;}
#header .userInfo li.info_logout a:before{background:url("../images/icon-logout.png") no-repeat center;}
#header .userInfo li.info_user span:before{background:url("../images/icon-user.png") no-repeat center;}

iframe {border: none; width: 100%; height:100%}
/* GNB */
.gnbArea {padding-left:220px; height:80px}
#header .gnb {clear:both; overflow:hidden; padding:15px 400px 0 50px;}
#header .gnb ul.nav {clear:both; overflow:hidden; }
#header .gnb ul.nav li {display:inline-block; margin:0 50px 0 0;}
#header .gnb ul.nav li a {display:block; padding:12px 0 21px; color:#333333; font-size:18px; font-weight: 400;}
#header .gnb ul.nav li a:hover,#header .gnb ul.nav li a:focus {color:#db4550; border-bottom: 5px solid #db4550;}
#header .gnb ul.nav li a.state-current {color:#db4550; border-bottom: 5px solid #db4550;}

/* container */
#page {display:block; height: calc(100vh - 81px); border-top: solid 1px #d9e2e7}
#container {/* clear:both; */ overflow:hidden; position:relative; /* padding:0 50px 0 0 */;height:100%; -moz-box-shadow: inset 0 1px 0 0 #d9e2e7; -webkit-box-shadow: inset 0 1px 0 0 #d9e2e7;box-shadow: inset 0 1px 0 0 #d9e2e7;}
 /* snb */
#snb { padding:0; background:#db4550;width:220px;height:100% ;  float:left; display: table-cell; min-height: 740px}
#snb #menu {clear:both; width:220px; margin-top: 64px;}
#snb #menu ul.nav {}
#snb #menu ul.nav li a:before{content: ""; width: 50px; height: 50px; display: block; margin: 30px auto -80px; position: relative; background: url("../images/icon-mnu.png") no-repeat 0 0;}
#snb #menu ul.nav li:hover a:before{background-position-x:-50px;}
#snb #menu ul.nav li.mnu-docu a:before{background-position-y:0;}
#snb #menu ul.nav li.mnu-certi a:before{background-position-y:-50px;}
#snb #menu ul.nav li.mnu-login a:before{background-position-y:-100px;}
#snb #menu ul.nav li.mnu-system a:before{background-position-y:-150px;}
#snb #menu ul.nav li.mnu-dash a:before{background-position-y:-200px;}
#snb #menu ul.nav > li:last-child {border:none;}
#snb #menu ul.nav > li > a {display:block; color:#fff; font-size:16px; font-weight: 700; text-align: center; height: 135px; line-height: 185px;border-top:1px solid rgba(255,255,255,0.3);border-bottom: 1px solid rgba(0,0,0,0.1)}
#snb #menu ul.nav > li > a:hover,#snb #menu ul.nav > li > a:focus {color:#fff; text-decoration:none; background:#fea43a;border-top:1px solid rgba(255,255,255,0.5);border-left:4px solid #fff;}
#snb #menu ul.nav > li.state-current > a {color:#fff;background: #fea43a;border-left:4px solid #fff;border-top:1px solid rgba(255,255,255,0.7);}
#snb #menu ul.nav > li.state-current > a:before{background-position-x:-50px;}

footer p { position: absolute; bottom:24px; color: rgba(255,255,255,0.5); font-size: 12px;line-height: 1.2; width: 220px; text-align: center; }

/* common align */
.text-l{text-align:left !important;}
.text-r{text-align:right !important;}
.text-c{text-align:center !important;}
.text-point{color:#ff6c6e;}

/* contents */
#contents {display:block;overflow:hidden; margin:0 50px; min-height:500px;padding-bottom: 50px;}
/* Form Element
================================== */
.search-pnl td > input:only-child, .search-pnl td > select:only-child {box-sizing: border-box; width:90%}
.grid-pnl td > input:only-child{box-sizing: border-box; width:80%}
td > input:only-child, td > select:only-child {box-sizing: border-box; width:100%}

input[type=text], input[type=password], input[type=email], input[type=tel], input[type=number]{
    text-align:left;
    background:#ffffff;
    -moz-border-radius:6px;
    -webkit-border-radius:6px;
    border-radius:6px;
    vertical-align:middle !important;
	border:1px solid #dedede;
	padding: 0 6px;
    height: 34px;
	line-height: 36px;
	outline: none;
}

select{
    background:#ffffff;
    -moz-border-radius:6px;
    -webkit-border-radius:6px;
    border-radius:6px;
    vertical-align:middle !important;
	border:1px solid #dedede;
	padding: 6px 30px 6px 6px;
	height: 36px;
	outline: none;
	background: url(../images/selectbox.png) no-repeat right 10px top 5px;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
}
select::-ms-expand {
	display: none;
}
textarea {
	background:#ffffff;
    -moz-border-radius:6px;
    -webkit-border-radius:6px;
    border-radius:6px;
    vertical-align:middle !important;
	border:1px solid #dedede;
	padding: 0 6px;
	line-height: 36px;
	outline: none;
	resize: none;
	margin: 7px 0;
}
.search-pnl input[type=text],
.search-pnl input[type=password],
.search-pnl input[type=email],
.search-pnl input[type=tel],
.search-pnl input[type=number]{
    height: 38px;
	line-height: 38px;
	font-size: 16px;
	font-weight: 400;
	padding: 0 8px;
}

.search-pnl select{ padding: 10px 30px 10px 8px;height: 40px;font-size: 16px;font-weight: 400;}

input[type=text][readonly], input[type=password][readonly], input[type=email][readonly], input[type=tel][readonly], input[type=number][readonly], textarea[readonly] { background-color: #f2f2f2 }
input[disabled], textarea[disabled] {background-color: #f2f2f2;}

select[disabled]{filter: grayscale(100%); background-color: #f2f2f2;}

/* checkbox & radio 마크업 시 Label이 꼭 필요함 */
input[type=checkbox] { position: relative; display: none;}
input[type=checkbox] + label  { position:relative;display: inline-block;width: 18px;height: 18px;  background:#fff; border:1px solid #aaa; border-radius: 4px; text-align: center; vertical-align: middle; margin: 0 8px;}
input[type=checkbox][disabled] + label  { border:1px solid #aaa;opacity: 0.3; }
input[type=checkbox] + label:after{content: "";display:block; position: relative ; height: 20px;background: url("../images/checked.png") no-repeat center 45%}
input[type=checkbox]:checked + label {background:#fea43a;border:1px solid #fea43a;}
input[type=checkbox][disabled]:checked + label {background-color:#aaa; border-color:#aaa}
input[type=checkbox]:checked + label:after {content: ""; color: #fff;}

input[type=radio] { position: relative; display: none;}
input[type=radio] + label  { position:relative;display: inline-block;width: 20px;height: 20px;  background:#fff; border:1px solid #aaa; border-radius: 10px; text-align: center; vertical-align: middle; margin: 0 8px;}
input[type=radio][disabled] + label  { border:1px solid #aaa; opacity: 0.3;}
input[type=radio]:checked + label {background:#fff;border:5px solid #fea43a;width: 12px;height: 12px; border-radius: 12px; }
input[type=radio][disabled]:checked + label {border-color:#aaa; }

input.dateinput{background:  url("../images/dateinput.png") no-repeat right center; padding-right: 24px!important;cursor:pointer;}
input.dateinput[disabled] {background-color: #f2f2f2;}
.select_group span{margin-right: 30px; }

/* buttons */
button{outline:none;cursor:pointer;text-align:center;text-decoration:none;background:none;vertical-align:middle ;  border:none;line-height:normal;-webkit-appearance:none;}
button{	height: 40px; line-height: 40px; padding: 0 40px; font-weight: 700; font-size: 16px; background: #fff; color: #000; border:1px solid #dedede; border-radius: 20px; }
button.state-highlight{ background: #fea43a; color: #fff; border:1px solid #fea43a;	}
button.state-submit{ background: #333333; color: #fff; border:1px solid #333333;	}
button:hover{}
.search-pnl td.buttonset button{ font-size: 18px; background: #fea43a; color: #fff; border:1px solid #fea43a;width: 145px; border-radius: 20px}
.search-pnl td.buttonset button:hover{  }

td button{background: #999; border-radius: 4px; color: #fff; font-size: 14px;padding: 0 24px;}
td button{background: #999; border-radius: 4px; color: #fff; font-size: 14px;padding: 0 24px;}
td button[class^=icon-] {width: 40px; border-radius: 20px;padding: 0; background:  url("../images/icon.png") no-repeat -5px -5px #999!important;}
td button[class^=image-] {width: 40px; border-radius: 20px;padding: 0; background:  url("../images/icon.png") no-repeat -56px -6px; border:none; }
button.image-folder{ background-position-x: -156px!important}
button.image-plus{ background-position-x: -206px!important}
button[class^=icon-]{padding: 0 10px 0 20px!important;width: auto;}
button.icon-plus:after{content: "";display: inline-block; width: 32px; height: 32px;  background:  url("../images/icon.png") no-repeat -60px -5px!important; float: right; position: relative; top:-1px; right: 0;}
button.icon-minus:after{content: "";display: inline-block; width: 32px; height: 32px;  background:  url("../images/icon.png") no-repeat -105px -5px!important; float: right; position: relative; top:-1px; right: 0;}
button[disabled] { cursor: default !important; -moz-opacity: .3; opacity: .3}
/* caption-pnl */
.caption-pnl {line-height: 40px;display: block; margin-bottom: 10px;margin-top: 60px;height: 40px;clear: both;}
.caption-pnl h2{font-size: 28px; font-weight: 700; float: left}
.caption-pnl h3{font-size: 20px; font-weight: 400; float: left}
.caption-pnl .buttonset{float: right;}
.caption-pnl:after, .table-pnl:after, .grid-pnl:after , .colgroup-wrap:after{display:block;visibility:hidden;height:0;font-size:0;clear:both;content:''}


/* section */
.section {position:relative; clear:both; overflow:hidden; margin:0 0 0 0;}

/* search-pnl */
.search-pnl {
	border: 1px solid #dedede;
	background: #fff;padding: 15px;
	margin: 10px 0 40px;
}
.search-pnl table{
    width:100%;
    table-layout:fixed;
	padding:0;
	border: none!important;
	background: transparent;
	font-size: 16px;
}
.search-pnl table th {text-align: left;border: none!important; padding-left: 30px;}
.search-pnl table td {border: none!important;}
.search-pnl table tr:first-child td:last-child {border-left: 1px solid #dedede!important; text-align: center}

/* grid-pnl */
.grid-pnl, .table-pnl{margin: 15px 0; clear: both}
.grid-pnl table{table-layout:fixed}
table{width: 100%;background: #fff; border: 1px solid #dedede;font-size: 14px; font-weight: 400; color: #14161b; }
table th{height: 50px;font-weight: 700; font-size: 16px; padding: 0 18px}
table tbody td, table tbody th{border-top:1px solid #ededed; height: 50px;}
table tbody td{	padding: 0 10px;}
table tbody tr:first-child td, table tbody tr:first-child th:first-child{border-top:none!important;}
table tbody tr.state-hover th, table tbody tr.state-hover td{background: #fef8eb;color: #ff6c6e;}
.grid-pnl tr:hover {background: #fef8eb;color: #ff6c6e;}
table.align-center td{text-align: center;}
table.align-right td{text-align: right;}
table tr.editable-td th, table tr.editable-td td{background: #fef8eb}

.grid-pnl thead th{background: #e8e8e8;color: #333; }
.table-pnl table{table-layout:fixed;}
.table-pnl table.vtable thead th{background: #fef8eb;color: #333; border-bottom: 1px solid #ededed; border-right: 1px solid #ededed; text-align: center}
.table-pnl table.vtable.bordertd td{ border-bottom: 1px solid #ededed; border-right: 1px solid #ededed;}

.table-pnl table.vtable thead tr td:last-child, .table-pnl table.vtable.bordertd tr td:last-child{ border-right: none;}
.table-pnl table.vtable.bordertd tr:last-child td{ border-bottom: none;}
.table-pnl th{background: #e8e8e8;color: #333;text-align: left;}
.table-pnl thead tr th:last-child{border-right: none;}
.table-pnl td textarea:only-child { box-sizing: border-box; width:100%;}

table strong:after{ content: "*"; display: inline-block; position: relative;; width: 12px; height: 12px; margin:0 0 0 6px;color: #da444f; top:-6px}
td span.dir_sub:before{ content:"";display: inline-block; position: relative;; width: 12px; height: 12px; margin:0 6px 0 0 ; background: url("../images/dir-sub.png") no-repeat center; }

.empty-pnl em{float: right; margin-bottom: 10px;}

/* colgroup (기존)*/
.colgroup-wrap{
    margin:10px 0 0 0;
}
/* colgroup (변경)*/
/* columns: 2 */
.colgroup-1-2,.colgroup-2-4,.colgroup-3-6,.colgroup-4-8,.colgroup-5-10,.colgroup-6-12,.colgroup-wrap .colgroup-wrap .colgroup-1-2,.colgroup-wrap .colgroup-wrap .colgroup-2-4,.colgroup-wrap .colgroup-wrap .colgroup-3-6,.colgroup-wrap .colgroup-wrap .colgroup-4-8,.colgroup-wrap .colgroup-wrap .colgroup-5-10,.colgroup-wrap .colgroup-wrap .colgroup-6-12{width:50%;}
/* columns: 3 */
.colgroup-1-3,.colgroup-2-6,.colgroup-3-9,.colgroup-4-12,.colgroup-wrap .colgroup-wrap .colgroup-1-3,.colgroup-wrap .colgroup-wrap .colgroup-2-6,.colgroup-wrap .colgroup-wrap .colgroup-3-9,.colgroup-wrap .colgroup-wrap .colgroup-4-12{width:33.33333%;}
.colgroup-2-3,.colgroup-4-6,.colgroup-6-9,.colgroup-8-12,.colgroup-wrap .colgroup-wrap .colgroup-2-3,.colgroup-wrap .colgroup-wrap .colgroup-4-6,.colgroup-wrap .colgroup-wrap .colgroup-6-9,.colgroup-wrap .colgroup-wrap .colgroup-8-12{width:66.66667%;}
/* columns: 4 */
.colgroup-1-4,.colgroup-2-8,.colgroup-3-12,.colgroup-wrap .colgroup-wrap .colgroup-1-4,.colgroup-wrap .colgroup-wrap .colgroup-2-8,.colgroup-wrap .colgroup-wrap .colgroup-3-12{width:25%;}
.colgroup-3-4,.colgroup-6-8,.colgroup-9-12,.colgroup-wrap .colgroup-wrap .colgroup-3-4,.colgroup-wrap .colgroup-wrap .colgroup-6-8,.colgroup-wrap .colgroup-wrap .colgroup-9-12{width:75%;}
/* columns: 5 */
.colgroup-1-5,.colgroup-2-10,.colgroup-wrap .colgroup-wrap .colgroup-1-5,.colgroup-wrap .colgroup-wrap .colgroup-2-10{width:20%;}
.colgroup-2-5,.colgroup-4-10,.colgroup-wrap .colgroup-wrap .colgroup-2-5,.colgroup-wrap .colgroup-wrap .colgroup-4-10{width:40%;}
.colgroup-3-5,.colgroup-6-10,.colgroup-wrap .colgroup-wrap .colgroup-3-5,.colgroup-wrap .colgroup-wrap .colgroup-6-10{width:60%;}
.colgroup-4-5,.colgroup-8-10,.colgroup-wrap .colgroup-wrap .colgroup-4-5,.colgroup-wrap .colgroup-wrap .colgroup-8-10{width:80%;}
/* columns: 6 */
.colgroup-1-6,.colgroup-2-12,.colgroup-wrap .colgroup-wrap .colgroup-1-6,.colgroup-wrap .colgroup-wrap .colgroup-2-12{width:16.66667%;}
.colgroup-5-6,.colgroup-10-12,.colgroup-wrap .colgroup-wrap .colgroup-5-6,.colgroup-wrap .colgroup-wrap .colgroup-10-12{width:83.33333%;}
/* columns: 7 */
.colgroup-1-7,.colgroup-wrap .colgroup-wrap .colgroup-1-7{width:14.28571%;}
.colgroup-2-7,.colgroup-wrap .colgroup-wrap .colgroup-2-7{width:28.57143%;}
.colgroup-3-7,.colgroup-wrap .colgroup-wrap .colgroup-3-7{width:42.85714%;}
.colgroup-4-7,.colgroup-wrap .colgroup-wrap .colgroup-4-7{width:57.14286%;}
.colgroup-5-7,.colgroup-wrap .colgroup-wrap .colgroup-5-7{width:71.42857%;}
.colgroup-6-7,.colgroup-wrap .colgroup-wrap .colgroup-6-7{width:85.71429%;}
/* columns: 8 */
.colgroup-1-8,.colgroup-wrap .colgroup-wrap .colgroup-1-8{width:12.5%;}
.colgroup-3-8,.colgroup-wrap .colgroup-wrap .colgroup-3-8{width:37.5%;}
.colgroup-5-8,.colgroup-wrap .colgroup-wrap .colgroup-5-8{width:62.5%;}
.colgroup-7-8,.colgroup-wrap .colgroup-wrap .colgroup-7-8{width:87.5%;}
/* columns: 9 */
.colgroup-1-9,.colgroup-wrap .colgroup-wrap .colgroup-1-9{width:11.11111%;}
.colgroup-2-9,.colgroup-wrap .colgroup-wrap .colgroup-2-9{width:22.22222%;}
.colgroup-4-9,.colgroup-wrap .colgroup-wrap .colgroup-4-9{width:44.44444%;}
.colgroup-5-9,.colgroup-wrap .colgroup-wrap .colgroup-5-9{width:55.55556%;}
.colgroup-7-9,.colgroup-wrap .colgroup-wrap .colgroup-7-9{width:77.77778%;}
.colgroup-8-9,.colgroup-wrap .colgroup-wrap .colgroup-8-9{width:88.88889%;}
/* columns: 10 */
.colgroup-1-10,.colgroup-wrap .colgroup-wrap .colgroup-1-10{width:10%;}
.colgroup-3-10,.colgroup-wrap .colgroup-wrap .colgroup-3-10{width:30%;}
.colgroup-7-10,.colgroup-wrap .colgroup-wrap .colgroup-7-10{width:70%;}
.colgroup-9-10,.colgroup-wrap .colgroup-wrap .colgroup-9-10{width:90%;}
/* columns: 11 */
.colgroup-1-11,.colgroup-wrap .colgroup-wrap .colgroup-1-11{width:9.09091%;}
.colgroup-2-11,.colgroup-wrap .colgroup-wrap .colgroup-2-11{width:18.18182%;}
.colgroup-3-11,.colgroup-wrap .colgroup-wrap .colgroup-3-11{width:27.27273%;}
.colgroup-4-11,.colgroup-wrap .colgroup-wrap .colgroup-4-11{width:36.36364%;}
.colgroup-5-11,.colgroup-wrap .colgroup-wrap .colgroup-5-11{width:45.45455%;}
.colgroup-6-11,.colgroup-wrap .colgroup-wrap .colgroup-6-11{width:54.54545%;}
.colgroup-7-11,.colgroup-wrap .colgroup-wrap .colgroup-7-11{width:63.63636%;}
.colgroup-8-11,.colgroup-wrap .colgroup-wrap .colgroup-8-11{width:72.72727%;}
.colgroup-9-11,.colgroup-wrap .colgroup-wrap .colgroup-9-11{width:81.81818%;}
.colgroup-10-11,.colgroup-wrap .colgroup-wrap .colgroup-10-11{width:90.90909%;}
/* columns: 12 */
.colgroup-1-12,.colgroup-wrap .colgroup-wrap .colgroup-1-12{width:8.33333%;}
.colgroup-5-12,.colgroup-wrap .colgroup-wrap .colgroup-5-12{width:41.66667%;}
.colgroup-7-12,.colgroup-wrap .colgroup-wrap .colgroup-7-12{width:58.33333%;}
.colgroup-11-12,.colgroup-wrap .colgroup-wrap .colgroup-11-12{width:91.66667%;}
.colgroup-wrap > div[class^=colgroup-]{float:left}

/* colgroup 간 간격 */
.colgroup-wrap > div[class^=colgroup-] > div {
    /* margin-left:5px; */
	margin: 0;
}
.colgroup-wrap > div[class^=colgroup-] + div[class^=colgroup-] > div {
	margin-left: 30px;
}

/* paging */
.paging {position:relative; clear:both;  margin:40px 0;text-align:center; font-size:13px;}
.paging a {color:#333; text-decoration:none;text-align:center; margin:0 2px; border:1px solid #dedede;background: #fff; border-radius: 4px;display: inline-block;min-width: 14px; height: 26px; line-height: 28px; padding: 0 6px;}
.paging a:hover, .paging a:focus, .paging a.state-current {color:#fff; border:1px solid #fea43a; background:#fea43a;}
/*추가*/
.paging strong {text-decoration:none;text-align:center; margin:0 2px; border-radius: 4px;display: inline-block;min-width: 14px; height: 26px; line-height: 28px; padding: 0 6px; color:#fff; border:1px solid #fea43a; background:#fea43a;}
/*추가*/

/* scroll */
.grid-pnl::-webkit-scrollbar {width: 25px;background-color: #999999;}
.grid-pnl::-webkit-scrollbar-thumb {background-color: #d3d3d3;}
.grid-pnl::-webkit-scrollbar-button {width: 25px;height: 25px;background-color: #7b7b7b;color:#f2f2f2;}
.grid-pnl::-webkit-scrollbar-button:start {background: url(../images/arrow_up.png) no-repeat center 6px;}
.grid-pnl::-webkit-scrollbar-button:end {background: url(../images/arrow_down.png) no-repeat center -2px}

/*추가*/
.table-pnl::-webkit-scrollbar {width: 25px;background-color: #999999;}
.table-pnl::-webkit-scrollbar-thumb {background-color: #d3d3d3;}
.table-pnl::-webkit-scrollbar-button {width: 25px;height: 25px;background-color: #7b7b7b;color:#f2f2f2;}
.table-pnl::-webkit-scrollbar-button:start {background: url(../images/arrow_up.png) no-repeat center 6px;}
.table-pnl::-webkit-scrollbar-button:end {background: url(../images/arrow_down.png) no-repeat center -2px}
/*추가*/

.section.dash .caption-pnl span{color:333; line-height:54px; margin-left:15px}

.section.dash .caption-pnl em{color:333; line-height:54px; float: right}
.section.dash .caption-pnl.text-c h2{ float:none; display: inline-block;}
.section.dash .caption-pnl.text-c select{ float: left;height: 40px;font-size: 16px;background-color: #ffffff; padding: 10px 40px 10px 8px;}
.section.dash .caption-pnl button.more{float: right; border:none; background:  url("../images/more.png") no-repeat center ; padding: 0 20px; border-radius: 0; }

.section.dash .vtable .text-c{font-weight:bold}
